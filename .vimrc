let g:solarized_contrast="high"

autocmd BufWritePre * :%s/\s\+$//e
filetype plugin on
set autoindent
set guifont=Monospace\ Bold\ 10
set guioptions=agi
set listchars=tab:→\ ,trail:·
set mouse=a
set mousefocus
set showtabline=2
set noequalalways
set noswapfile
set nowrap
set number
set shiftwidth=2
set smartindent
set tabstop=2
set winminheight=0
set winminwidth=0
set showcmd
set background=light
syntax on
